package domain;

public class Award {

    private AwardId id;

    private Edition edition;

    public Award() {}

    public Award(AwardId id, Edition edition) {
        this.id = id;
        this.edition = edition;
    }

    public AwardId getId() {
        return id;
    }

    public void setId(AwardId id) {
        this.id = id;
    }

    public Edition getEdition() {
        return edition;
    }

    public void setEdition(Edition edition) {
        this.edition = edition;
    }
}
