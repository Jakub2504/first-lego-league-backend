package domain;

import java.io.Serializable;
import java.util.Objects;

public class AwardId implements Serializable {

    private String name;
    private Integer editionNumber;

    public AwardId() {}

    public AwardId(String name, Integer editionNumber) {
        this.name = name;
        this.editionNumber = editionNumber;
    }

    public String getName() {
        return name;
    }

    public Integer getEditionNumber() {
        return editionNumber;
    }

    @Override
    public boolean equals(Object o) {
        if (this == o) return true;
        if (!(o instanceof AwardId)) return false;
        AwardId awardId = (AwardId) o;
        return Objects.equals(name, awardId.name) &&
               Objects.equals(editionNumber, awardId.editionNumber);
    }

    @Override
    public int hashCode() {
        return Objects.hash(name, editionNumber);
    }
}
